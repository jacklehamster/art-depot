{
  "name": "cloudflare-worker",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "copy-wasm": "node scripts/copy-wasm.mjs",
    "dev": "bun run copy-wasm && wrangler dev src/index.ts --config wrangler.dev.toml",
    "deploy": "bun run copy-wasm && wrangler deploy",
    "build": "bun i && bun run copy-wasm && tsc"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "latest",
    "typescript": "latest",
    "wrangler": "latest"
  },
  "dependencies": {
    "@jsquash/avif": "^2.1.1",
    "@jsquash/jpeg": "^1.6.0",
    "@jsquash/png": "^3.1.1",
    "@jsquash/webp": "^1.5.0",
    "gulp-avif": "^1.1.1"
  }
}
